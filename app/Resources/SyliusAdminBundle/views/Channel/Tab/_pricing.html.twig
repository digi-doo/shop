<div class="ui tab" data-tab="pricing">
    <h3 class="ui dividing header">{{ 'sylius.ui.channel_pricing'|trans }}</h3>    
    <div class="column">
        {{ form_row(form.currencies) }}
        {{ form_row(form.baseCurrency) }}
        {{ form_row(form.defaultTaxZone) }}
        {{ form_row(form.taxCalculationStrategy) }}
        {{ form_row(form.freeShippingFrom) }}
    </div>
    {{ sonata_block_render_event('sylius.admin.channel.' ~ action ~ '.tab_settings', {'form': form}) }}
</div>

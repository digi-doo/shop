<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>{% block title %}{{ tenant_name }}{% endblock %}</title>

    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    {% block metatags %}
        {% set version = '001' %}
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('assets/shop/img/favicon/apple-touch-icon.png?v=' ~ version) }}">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('assets/shop/img/favicon/favicon-32x32.png?v=' ~ version) }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('assets/shop/img/favicon/favicon-16x16.png?v=' ~ version) }}">
        <meta name="theme-color" content="#ffffff">
    {% endblock %}

    {% block stylesheets %}
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

    {# jQuery CDN #}
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
</head>
<body class="centered {% block bodyClass '' %}">
{% block pre_content %}
{% endblock %}

{% block content %}
{% endblock %}

{% block post_content %}
{% endblock %}

{% block javascripts %}
{% endblock %}
</body>
</html>
